{
    "uid": "https://github.com/pinf/server-js/",
    "name": "server-js",
    "main": "lib/server.js",
    "engine": [
        "node"
    ],
    "repositories": [
        {
            "type": "git",
            "url": "git://github.com/pinf/server-js.git"
        }
    ],
    "scripts": {
        "serve": {
            "location": "./",
            "module": "/scripts/serve.js"
        }
    },
    "mappings": {
        "nodejs": {
            "id": "nodejs.org"
        },
        "pinf": {
            "id": "pinf.org/loader"
        },
        "connect": {
            "id": "github.com/senchalabs/connect/"
        },
        "console": {
            "id": "github.com/pinf/console-js/"
        },
        "modules": {
            "id": "github.com/pinf/modules-js/"
        },
        "formidable": {
            "id": "github.com/felixge/node-formidable/"
        }
    }
}